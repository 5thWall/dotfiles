# ZSH Home
ZSH=$HOME/.oh-my-zsh

# ZSH theme
ZSH_THEME="juanghurtado"

# Oh-My-ZSH plugins
plugins=(git github brew ruby pow osx)

source $ZSH/oh-my-zsh.sh

# Custom
for file in ~/.zsh/{exports,aliases,functions,completion}.zsh; do
	[ -r $file ] && source $file
done
unset file

# Include secrets...
source ~/.secret
